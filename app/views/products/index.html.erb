<%= render "product_mng_menu" %>
<table id="all_products">
  <tr>
    <td class="main">
      <span><%=t(:products)%></span>
    </td>
  </tr>
  <% unless @store.products.empty? %>
      <% @products.each do |product| %>
          <tr>
            <td><%= link_to product.name, "/products/" + product.id.to_s %></td>
          </tr>
      <% end %>
  <% end %>
</table>
