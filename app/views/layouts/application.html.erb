<!DOCTYPE html>
<html>
<head>
  <title><%= title %></title>
  <%= stylesheet_link_tag :all %>
  <%= javascript_include_tag 'jquery.min', 'jquery_ujs', 'jquery-ui.min', 'jquery.ui.potato.menu',
                             'jquery.equal-heights', 'application' %>
  <%= csrf_meta_tag %>
</head>
<body>

<div id="container">
  <%= render :partial=>'layouts/header' %>

  <%= render :partial=> 'layouts/flash_msgs' %>

  <div id="content">
    <%= yield %>
  </div>

  <%= render 'layouts/footer' %>

  <!--<%= debug(params) if Rails.env.development? %>-->
</div>

</body>
</html>
